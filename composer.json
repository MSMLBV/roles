{
    "name": "ultraware/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 5.",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "7.*",
        "illuminate/database": "7.*"
    },
    "autoload": {
        "psr-4": {
            "Ultraware\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "5.1.0",
        "orchestra/database": "^5.0.0",
        "mockery/mockery": "^1.0",
        "sebastian/environment": "^4.2.2",
        "phpunit/phpunit": "~8.5",
        "satooshi/php-coveralls": "^2.0"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ultraware\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
